{
  "name": "mqtt-logging-client",
  "version": "1.0.0",
  "description": "MQTT client to subscribe to MQTT broker and log all messages to DB",
  "main": "src/app.js",
  "scripts": {
    "test": "mocha './test/**/*.specs.js'",
    "test:watch": "npm test -- -w --recursive --watch-extensions js",
    "test:cover": "nyc --reporter=lcov npm test",
    "test:cover:report": "nyc mocha './test/**/*.specs.js' && nyc report --reporter=text-lcov | codacy-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramunsk/mqtt-logging-client.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramunsk/mqtt-logging-client/issues"
  },
  "homepage": "https://github.com/ramunsk/mqtt-logging-client#readme",
  "dependencies": {
    "mongoose": "4.8.1",
    "mqtt": "2.3.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codacy-coverage": "2.0.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^10.1.2",
    "proxyquire": "1.7.11",
    "sinon": "1.17.7"
  }
